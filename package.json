{
  "name": "pokemon-tcg-deck-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && BUILDING=true ESLINT_NO_DEV_ERRORS=true next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testMatch='**/*.test.{ts,tsx}'",
    "test:integration": "jest --testMatch='**/*.integration.test.{ts,tsx}'",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "script:ensure-user": "tsx src/scripts/ensure-user-exists.ts",
    "script:debug-user": "tsx src/scripts/debug-user-creation.ts",
    "script:quick-fix": "tsx src/scripts/quick-user-fix.ts",
    "script:test-deck": "tsx src/scripts/test-deck-creation.ts",
    "analyzer:feedback": "tsx src/scripts/run-analyzer-feedback-loop.ts",
    "redis:test": "tsx src/scripts/test-redis-connection.ts",
    "worker:ai": "npx tsx src/scripts/ai-analysis-worker.ts",
    "worker:ai:local": "npx dotenv -e .env.local -- npx tsx src/scripts/ai-analysis-worker.ts",
    "worker:all": "npx dotenv -e .env.local -- npx tsx src/scripts/start-workers.ts",
    "test:redis": "npx dotenv -e .env.local -- npx tsx src/scripts/test-redis-connection.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.22.0",
    "@clerk/themes": "^2.2.50",
    "@headlessui/react": "^2.2.4",
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.10.1",
    "@stripe/stripe-js": "^7.3.1",
    "@tanstack/react-query": "^5.81.2",
    "@trpc/client": "^11.4.2",
    "@trpc/next": "^11.4.2",
    "@trpc/react-query": "^11.4.2",
    "@trpc/server": "^11.4.2",
    "@types/cheerio": "^0.22.35",
    "@upstash/redis": "^1.35.0",
    "axios": "^1.10.0",
    "bull": "^4.16.5",
    "bullmq": "^5.55.0",
    "chart.js": "^4.5.0",
    "cheerio": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.19.1",
    "ioredis": "^5.6.1",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.522.0",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "p-retry": "^6.2.1",
    "papaparse": "^5.5.1",
    "prisma": "^6.10.1",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "redis": "^5.5.6",
    "stripe": "^18.2.1",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^4.2.4",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bull": "^3.15.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
